      <Button fx:id="btnExit" layoutX="234.0" layoutY="111.0" mnemonicParsing="false" onAction="#exitGame" prefHeight="25.0" prefWidth="90.0" text="Exit" />
      <Label layoutX="71.0" layoutY="57.0" prefHeight="17.0" prefWidth="37.0" text="Level:" />
      <Label alignment="CENTER" layoutX="100.0" layoutY="21.0" prefHeight="17.0" prefWidth="150.0" text="Memory Master" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="lblError" layoutX="14.0" layoutY="126.0" prefHeight="37.0" prefWidth="90.0" />
